/* =========================================
   CIRCUIT STUDIO - CLEAN REBUILD
   ========================================= */

:root {
    --bg-0: #0b0f14;
    --bg-1: #0f1621;
    --bg-2: #111a26;
    --panel: #0e151f;
    --panel-strong: #121b28;
    --field-bg: #3a3a3a;
    --field-border: #3a3a3a;
    --line: rgba(255, 255, 255, 0.06);
    --line-strong: rgba(255, 255, 255, 0.12);
    --text-0: #f8fafc;
    --text-1: #cbd5f5;
    --text-2: #94a3b8;
    --accent: #38bdf8;
    --accent-2: #f59e0b;
    --ok: #34d399;
    --warn: #fbbf24;
    --bad: #f87171;
}

.studio-container {
    flex-grow: 1;
    flex-direction: column;
    background-color: var(--bg-0);
    color: var(--text-1);
    width: 100%;
    height: 100%;
    min-width: 0;
    min-height: 0;
}

.studio-compact .top-menubar {
    height: 36px;
    padding-left: 10px;
    padding-right: 10px;
}

.studio-compact .studio-toolbar {
    height: 46px;
    padding-left: 10px;
    padding-right: 10px;
}

.studio-compact .menubar-left {
    min-width: 0;
    max-width: none;
}

.studio-compact .menu-btn {
    font-size: 10px;
    padding: 4px 6px;
}

.studio-compact .branding-text {
    font-size: 11px;
}

.studio-compact .branding-sub {
    font-size: 9px;
    margin-left: 6px;
}

.studio-compact .workspace-area {
    flex-direction: column;
}

.studio-compact .panel-left,
.studio-compact .panel-right {
    width: 100%;
    min-width: 0;
    max-width: none;
    border-left-width: 0;
    border-right-width: 0;
}

.studio-compact .panel-right {
    margin-top: 10px;
}

.studio-compact .center-header {
    height: 32px;
}

.studio-compact .panel-header {
    padding: 10px 12px;
}

.studio-compact .panel-title {
    font-size: 11px;
}

.studio-compact .tab-btn {
    font-size: 9px;
    padding: 4px 6px;
}

.studio-medium .top-menubar {
    height: 40px;
}

.studio-medium .studio-toolbar {
    height: 50px;
}

.studio-medium .panel-left {
    width: 260px;
    min-width: 220px;
    max-width: 420px;
}

.studio-medium .panel-right {
    width: 280px;
    min-width: 240px;
    max-width: 300px;
}

.top-menubar {
    height: 42px;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    padding-left: 16px;
    padding-right: 16px;
    background-color: var(--bg-1);
    border-bottom-width: 1px;
    border-bottom-color: var(--line);
    flex-shrink: 0;
}

.menubar-left,
.menubar-center,
.menubar-right {
    flex-direction: row;
    align-items: center;
}

.menubar-left,
.menubar-right {
    flex-shrink: 0;
}

.menubar-left {
    min-width: 240px;
    max-width: 360px;
    overflow: hidden;
    align-items: center;
}

.menubar-center {
    flex-grow: 1;
    justify-content: center;
    min-width: 0;
}

.branding-text {
    color: var(--text-0);
    font-size: 13px;
    -unity-font-style: bold;
    letter-spacing: 1px;
    white-space: nowrap;
    overflow: hidden;
    -unity-text-overflow-position: end;
}

.branding-sub {
    color: var(--accent);
    font-size: 10px;
    margin-left: 10px;
    letter-spacing: 1.2px;
    white-space: nowrap;
    overflow: hidden;
    -unity-text-overflow-position: end;
}

.brand-icon {
    width: 18px;
    height: 18px;
    margin-right: 8px;
    -unity-background-image-tint-color: white;
    -unity-background-scale-mode: scale-to-fit;
}

.icon-hexagon {
    background-image: url('/Assets/UI/Icons/hexagon.png');
}

.menu-btn {
    background-color: transparent;
    border-top-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    border-right-width: 0;
    color: var(--text-2);
    font-size: 11px;
    padding: 6px 10px;
}

.menu-btn:hover {
    color: var(--text-0);
}

.status-indicator {
    flex-direction: row;
    align-items: center;
    margin-right: 12px;
    background-color: rgba(56, 189, 248, 0.12);
    padding: 4px 8px;
    border-radius: 999px;
}

.status-dot {
    width: 6px;
    height: 6px;
    background-color: var(--ok);
    border-radius: 3px;
    margin-right: 6px;
}

.status-indicator Label {
    color: var(--text-1);
    font-size: 9px;
    -unity-font-style: bold;
}

.version-text {
    color: var(--text-2);
    font-size: 10px;
}

.studio-toolbar {
    height: 54px;
    background-color: var(--bg-2);
    border-bottom-width: 1px;
    border-bottom-color: var(--line);
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    padding-left: 16px;
    padding-right: 16px;
    flex-shrink: 0;
}

.tool-cluster {
    flex-direction: row;
    align-items: center;
}

.toolbar-btn {
    height: 32px;
    min-width: 52px;
    background-color: var(--panel-strong);
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: var(--line-strong);
    border-bottom-color: var(--line-strong);
    border-left-color: var(--line-strong);
    border-right-color: var(--line-strong);
    border-radius: 6px;
    color: var(--text-1);
    font-size: 10px;
    -unity-font-style: bold;
    margin-right: 6px;
}

.toolbar-btn.active {
    background-color: var(--accent);
    color: #0b0f14;
    border-top-color: var(--accent);
    border-bottom-color: var(--accent);
    border-left-color: var(--accent);
    border-right-color: var(--accent);
}

.toolbar-actions {
    flex-direction: row;
    align-items: center;
}

.toolbar-chip {
    background-color: transparent;
    color: var(--accent);
    border-radius: 999px;
    padding: 4px 10px;
    font-size: 9px;
    margin-right: 8px;
}

.btn-secondary {
    background-color: transparent;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: var(--accent);
    border-bottom-color: var(--accent);
    border-left-color: var(--accent);
    border-right-color: var(--accent);
    color: var(--accent);
    border-radius: 6px;
    padding: 8px 14px;
    font-size: 11px;
    -unity-font-style: bold;
    margin-right: 8px;
}

.btn-primary {
    background-color: var(--accent-2);
    color: #0b0f14;
    border-radius: 6px;
    padding: 8px 16px;
    font-size: 11px;
    -unity-font-style: bold;
}

.workspace-area {
    flex-grow: 1;
    flex-direction: row;
    min-height: 0;
    min-width: 0;
    overflow: hidden;
}

.center-area {
    flex-grow: 1;
    flex-direction: column;
    min-width: 0;
    min-height: 0;
    overflow: hidden;
}

.center-header {
    height: 36px;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    padding-left: 12px;
    padding-right: 12px;
    background-color: var(--bg-1);
    border-bottom-width: 1px;
    border-bottom-color: var(--line);
    flex-shrink: 0;
}

.center-tabs {
    flex-direction: row;
    align-items: center;
}

.center-tab-btn {
    background-color: transparent;
    color: var(--text-2);
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: var(--line-strong);
    border-bottom-color: var(--line-strong);
    border-left-color: var(--line-strong);
    border-right-color: var(--line-strong);
    border-radius: 6px;
    font-size: 9px;
    padding: 4px 10px;
    margin-right: 6px;
}

.center-tab-btn.active {
    color: #0b0f14;
    background-color: var(--accent-2);
    border-top-color: var(--accent-2);
    border-bottom-color: var(--accent-2);
    border-left-color: var(--accent-2);
    border-right-color: var(--accent-2);
}

.center-chip {
    background-color: rgba(148, 163, 184, 0.15);
    color: var(--text-1);
    border-radius: 6px;
    padding: 4px 8px;
    font-size: 9px;
}

.center-content {
    flex-grow: 1;
    min-height: 0;
    min-width: 0;
    flex-direction: column;
}

.panel-left,
.panel-right {
    background-color: var(--panel);
    flex-direction: column;
    border-right-width: 1px;
    border-right-color: var(--line);
    border-left-width: 1px;
    border-left-color: var(--line);
    flex-shrink: 0;
    min-height: 0;
}

.panel-left {
    width: 300px;
    min-width: 240px;
    max-width: 520px;
}

.panel-right {
    width: 320px;
    min-width: 320px;
    max-width: 320px;
    flex-grow: 0;
    flex-shrink: 0;
    overflow: hidden;
}

.panel-header {
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    padding: 12px 14px;
    border-bottom-width: 1px;
    border-bottom-color: var(--line);
    flex-shrink: 0;
    min-height: 36px;
}

.panel-title {
    color: var(--text-0);
    font-size: 12px;
    -unity-font-style: bold;
}

.panel-tabs {
    flex-direction: row;
    align-items: center;
}

.tab-btn {
    background-color: transparent;
    color: var(--text-2);
    font-size: 10px;
    border-top-width: 0;
    border-bottom-width: 2px;
    border-left-width: 0;
    border-right-width: 0;
    border-bottom-color: transparent;
    padding: 6px 8px;
}

.tab-btn.active {
    color: var(--text-0);
    border-bottom-color: var(--accent-2);
}

.panel-content {
    flex-grow: 1;
    min-height: 0;
}

.left-panel-body {
    flex-grow: 1;
    min-height: 120px;
    flex-direction: column;
    padding-top: 6px;
    padding-left: 6px;
    padding-right: 16px;
    padding-bottom: 6px;
}

.tree-root {
    padding: 12px;
    padding-right: 0;
}

.tree-item {
    color: var(--text-1);
    font-size: 11px;
    padding: 4px 6px;
}

.tree-item:hover {
    background-color: rgba(148, 163, 184, 0.12);
    border-radius: 6px;
}

.root-item {
    color: var(--text-0);
    -unity-font-style: bold;
}

.library-panel {
    height: 300px;
    min-height: 200px;
    max-height: 320px;
    border-top-width: 1px;
    border-top-color: rgba(245, 158, 11, 0.4);
    background-color: var(--bg-0);
    padding: 8px 16px 8px 4px;
    flex-shrink: 0;
    flex-grow: 0;
    flex-direction: column;
}

.panel-header-sm {
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 8px;
}

.panel-header-sm Label {
    color: var(--text-0);
    font-size: 11px;
    -unity-font-style: bold;
}

.btn-icon {
    width: 22px;
    height: 22px;
    background-color: var(--accent-2);
    color: #0b0f14;
    border-radius: 4px;
    -unity-text-align: middle-center;
    font-size: 14px;
    padding: 0;
}

.search-field {
    height: 28px;
    min-height: 28px;
    max-height: 28px;
    background-color: var(--field-bg);
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: var(--field-border);
    border-bottom-color: var(--field-border);
    border-left-color: var(--field-border);
    border-right-color: var(--field-border);
    border-radius: 6px;
    font-size: 11px;
    color: #ffffff;
    padding-left: 8px;
    padding-right: 8px;
    flex-shrink: 0;
    flex-grow: 0;
}

.library-panel .search-field {
    background-color: var(--field-bg);
    height: 28px;
}

.search-field .unity-text-input {
    color: #ffffff;
    background-color: transparent;
}

.search-field .unity-text-field__input,
.search-field .unity-base-text-field__input,
.search-field .unity-text-field__input--single-line,
.search-field .unity-base-text-field__input--single-line {
    background-color: transparent;
    color: #ffffff;
    border-width: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 4px;
}

.search-field .unity-text-field,
.search-field .unity-base-text-field {
    background-color: transparent;
    border-width: 0;
}

.search-field.unity-text-field,
.search-field.unity-base-text-field {
    background-color: transparent;
    border-width: 0;
}

.library-list {
    flex-grow: 1;
    margin-top: 8px;
    min-height: 0;
}

.lib-item {
    flex-direction: row;
    align-items: center;
    padding: 6px 8px;
    margin-bottom: 6px;
    background-color: var(--panel-strong);
    border-radius: 8px;
    min-height: 44px;
    min-width: 0;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: var(--line);
    border-bottom-color: var(--line);
    border-left-color: var(--line);
    border-right-color: var(--line);
}

.lib-icon-box {
    width: 32px;
    height: 32px;
    background-color: rgba(255, 255, 255, 0.06);
    border-radius: 6px;
    align-items: center;
    justify-content: center;
    margin-right: 8px;
    flex-shrink: 0;
}

.lib-icon-box Label {
    color: var(--text-0);
    font-size: 11px;
    -unity-font-style: bold;
}

.lib-item-info {
    min-width: 0;
    flex-grow: 1;
}

.lib-item-title,
.lib-title {
    color: var(--text-0);
    font-size: 11px;
    -unity-font-style: bold;
    -unity-text-align: middle-left;
    white-space: nowrap;
    overflow: hidden;
    -unity-text-overflow-position: end;
}

.lib-item-sub,
.lib-sub {
    color: var(--text-2);
    font-size: 9px;
    -unity-text-align: middle-left;
    white-space: nowrap;
    overflow: hidden;
    -unity-text-overflow-position: end;
}

.canvas-view {
    flex-grow: 1;
    background-color: var(--bg-0);
    overflow: hidden;
    min-width: 0;
    padding: 12px;
    flex-shrink: 1;
    min-height: 0;
}

.canvas-surface {
    flex-grow: 1;
    background-color: rgba(11, 17, 26, 0.2);
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: rgba(56, 189, 248, 0.25);
    border-bottom-color: rgba(56, 189, 248, 0.25);
    border-left-color: rgba(56, 189, 248, 0.25);
    border-right-color: rgba(56, 189, 248, 0.25);
    border-radius: 10px;
    position: relative;
    overflow: hidden;
    min-height: 0;
}

.canvas-selection-box {
    position: absolute;
    border-width: 1px;
    border-color: rgba(59, 130, 246, 0.9);
    background-color: rgba(59, 130, 246, 0.18);
    border-radius: 6px;
    display: none;
}

.canvas-grid {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background-image: resource("UI/grid_512");
    -unity-background-scale-mode: scale-and-crop;
    opacity: 0.35;
}

.canvas-grid-layer {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
}

.canvas-hud {
    position: absolute;
    right: 12px;
    bottom: 12px;
    background-color: rgba(15, 23, 42, 0.8);
    padding: 6px 10px;
    border-radius: 6px;
}

.hud-label {
    color: var(--text-2);
    font-size: 10px;
}

.canvas-text-note {
    min-width: 80px;
    max-width: 240px;
    background-color: #1b222b;
    color: #e8eef5;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: #2b3440;
    border-bottom-color: #2b3440;
    border-left-color: #2b3440;
    border-right-color: #2b3440;
    border-radius: 6px;
    padding: 4px 6px;
    font-size: 10px;
}

.canvas-text-note .unity-text-input {
    color: #e8eef5;
    background-color: transparent;
}

.canvas-text-note .unity-text-field__input,
.canvas-text-note .unity-base-text-field__input,
.canvas-text-note .unity-text-field__input--single-line,
.canvas-text-note .unity-base-text-field__input--single-line {
    background-color: transparent;
    color: #e8eef5;
    border-width: 0;
}

.text-note {
    background-color: rgba(255, 255, 255, 0.08);
    border-radius: 6px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: rgba(255, 255, 255, 0.18);
    border-bottom-color: rgba(255, 255, 255, 0.18);
    border-left-color: rgba(255, 255, 255, 0.18);
    border-right-color: rgba(255, 255, 255, 0.18);
    padding: 4px;
}

.text-note-field {
    flex-grow: 1;
    min-width: 0;
    min-height: 0;
    background-color: transparent;
    color: var(--text-0);
    border-width: 0;
}

.text-note-field .unity-text-field__input,
.text-note-field .unity-base-text-field__input,
.text-note-field .unity-text-field__input--multiline,
.text-note-field .unity-base-text-field__input--multiline {
    background-color: transparent;
    color: var(--text-0);
    border-width: 0;
    padding: 4px 6px;
}

.circuit-component {
    background-color: #111925;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: rgba(56, 189, 248, 0.25);
    border-bottom-color: rgba(56, 189, 248, 0.25);
    border-left-color: rgba(56, 189, 248, 0.25);
    border-right-color: rgba(56, 189, 248, 0.25);
    border-radius: 8px;
    padding: 6px;
    color: #e1e8f2;
}

.circuit-component.selected {
    border-top-color: #38bdf8;
    border-bottom-color: #38bdf8;
    border-left-color: #38bdf8;
    border-right-color: #38bdf8;
}

.custom-component {
    background-color: #151b25;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: rgba(56, 189, 248, 0.2);
    border-bottom-color: rgba(56, 189, 248, 0.2);
    border-left-color: rgba(56, 189, 248, 0.2);
    border-right-color: rgba(56, 189, 248, 0.2);
    border-radius: 8px;
    overflow: visible;
}

.custom-component-body {
    flex-grow: 1;
    position: relative;
}

.custom-pin-dot {
    width: 10px;
    height: 10px;
    border-radius: 5px;
    background-color: #38bdf8;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: rgba(8, 20, 32, 0.8);
    border-bottom-color: rgba(8, 20, 32, 0.8);
    border-left-color: rgba(8, 20, 32, 0.8);
    border-right-color: rgba(8, 20, 32, 0.8);
}

.custom-pin-label {
    color: #9bb0c6;
    font-size: 9px;
}

.custom-component-label {
    color: #e6eefb;
    font-size: 10px;
}

.custom-shape {
    position: absolute;
}

.custom-shape-text {
    color: #e6eefb;
    font-size: 9px;
    -unity-text-align: middle-center;
    white-space: nowrap;
}

.block-component {
    background-color: #0f1621;
}

.block-row {
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    min-height: 40px;
}

.block-pin {
    width: 56px;
    align-items: center;
    justify-content: center;
}

.block-pin-left {
    margin-left: -10px;
}

.block-pin-right {
    margin-right: -10px;
}

.block-pin-left .block-pin-dot {
    margin-left: -10px;
}

.block-pin-right .block-pin-dot {
    margin-right: -10px;
}

.block-pin-label {
    color: #9bb0c6;
    font-size: 10px;
    width: 40px;
    -unity-text-align: middle-center;
}

.block-pin-label.left {
    -unity-text-align: middle-left;
}

.block-pin-label.right {
    -unity-text-align: middle-right;
}

.block-pin-dot {
    width: 10px;
    height: 10px;
    border-radius: 5px;
    background-color: #38bdf8;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: rgba(8, 20, 32, 0.8);
    border-bottom-color: rgba(8, 20, 32, 0.8);
    border-left-color: rgba(8, 20, 32, 0.8);
    border-right-color: rgba(8, 20, 32, 0.8);
    margin-top: 2px;
    margin-bottom: 2px;
}

.block-body {
