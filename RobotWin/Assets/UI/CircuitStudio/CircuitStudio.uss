.unity-scroll-view__horizontal-scroller .unity-base-slider__low-button,
.unity-scroll-view__horizontal-scroller .unity-base-slider__high-button {
    display: none;
    width: 0;
    height: 0;
    padding: 0;
    margin: 0;
    border-width: 0;
    background-image: none;
    -unity-background-image-tint-color: transparent;
    opacity: 0;
    visibility: hidden;
}

.unity-scroller__arrow,
.unity-scroller__button,
.unity-scroll-view__vertical-scroller .unity-base-slider__low-button,
.unity-scroll-view__vertical-scroller .unity-base-slider__high-button,
.unity-scroll-view__horizontal-scroller .unity-base-slider__low-button,
.unity-scroll-view__horizontal-scroller .unity-base-slider__high-button {
    width: 0;
    height: 0;
}

.unity-scroll-view__vertical-scroller:hover .unity-base-slider__dragger,
.unity-scroll-view__horizontal-scroller:hover .unity-base-slider__dragger {
    background-color: #4a4a4a;
}

.circuit-3d-panel {
    flex-grow: 1;
    padding: 10px 12px;
    min-height: 0;
    position: relative;
}

.circuit-3d-view {
    flex-grow: 1;
    background-color: var(--bg-0);
    position: relative;
    border-radius: 8px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: var(--line);
    border-bottom-color: var(--line);
    border-left-color: var(--line);
    border-right-color: var(--line);
    align-items: center;
    justify-content: center;
    -unity-background-scale-mode: scale-to-fit;
}

.circuit-3d-loading {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    border-radius: 8px;
    background-color: rgba(5, 7, 12, 0.55);
    justify-content: center;
    align-items: center;
    -unity-background-scale-mode: scale-and-crop;
}

.circuit-3d-loading-card {
    background-color: rgba(18, 22, 31, 0.92);
    border-radius: 10px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: var(--line-strong);
    border-bottom-color: var(--line-strong);
    border-left-color: var(--line-strong);
    border-right-color: var(--line-strong);
    padding: 14px 18px;
    min-width: 240px;
    align-items: center;
    justify-content: center;
}

.circuit-3d-loading-spinner {
    width: 26px;
    height: 26px;
    border-radius: 13px;
    border-top-width: 3px;
    border-bottom-width: 3px;
    border-left-width: 3px;
    border-right-width: 3px;
    border-top-color: rgba(56, 189, 248, 0.95);
    border-right-color: rgba(56, 189, 248, 0.28);
    border-bottom-color: rgba(56, 189, 248, 0.28);
    border-left-color: rgba(56, 189, 248, 0.28);
    margin-bottom: 10px;
}

.circuit-3d-loading-title {
    font-size: 12px;
    -unity-font-style: bold;
    color: var(--text-0);
}

.board-3d-help {
    position: absolute;
    top: 12px;
    left: 12px;
    width: 32px;
    height: 32px;
    border-radius: 16px;
    border-width: 1px;
    border-color: rgba(255, 255, 255, 0.15);
    background-color: rgba(8, 12, 18, 0.85);
    align-items: center;
    justify-content: center;
    flex-direction: column;
    padding: 4px;
    display: flex;
}

.board-3d-help-icon {
    color: #f1f5f9;
    font-size: 16px;
    padding-left: 5px;
    padding-bottom: 5px;
    -unity-text-align: middle-center;
    -unity-font-style: bold;
}

.board-3d-help-tooltip {
    position: absolute;
    top: 40px;
    left: 0;
    width: 220px;
    padding: 8px;
    border-radius: 8px;
    border-width: 1px;
    border-color: rgba(148, 163, 184, 0.4);
    background-color: rgba(5, 7, 12, 0.95);
    flex-direction: column;
    display: none;
    /* Unity USS does not recognize z-index or pointer-events */
}

.board-3d-help-title {
    color: #38bdf8;
    font-size: 10px;
    margin-bottom: 4px;
    -unity-font-style: bold;
}

.board-3d-help-line {
    color: #cbd5f5;
    font-size: 10px;
    margin-bottom: 2px;
}

.circuit-3d-label {
    color: var(--text-2);
    font-size: 11px;
}

.circuit-3d-controls {
    position: absolute;
    top: 10px;
    right: 10px;
    width: 220px;
    padding: 8px 10px;
    border-radius: 8px;
    background-color: rgba(9, 12, 18, 0.72);
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: var(--line-strong);
    border-bottom-color: var(--line-strong);
    border-left-color: var(--line-strong);
    border-right-color: var(--line-strong);
}

.circuit-3d-controls-header {
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 6px;
}

.circuit-3d-controls-title {
    font-size: 10px;
    color: var(--text-2);
}

.circuit-3d-controls-toggle {
    font-size: 9px;
    color: var(--text-1);
    background-color: rgba(16, 20, 28, 0.7);
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: var(--line-strong);
    border-bottom-color: var(--line-strong);
    border-left-color: var(--line-strong);
    border-right-color: var(--line-strong);
    padding: 2px 6px;
    border-radius: 6px;
}

.circuit-3d-controls-body {
    flex-direction: column;
}

.circuit-3d-controls-subtitle {
    font-size: 10px;
    color: var(--text-2);
    margin-bottom: 6px;
}

.circuit-3d-controls.controls-collapsed {
    padding-bottom: 6px;
}

.circuit-3d-controls-row {
    flex-direction: row;
    align-items: center;
    margin-bottom: 6px;
}

.circuit-3d-controls-label {
    font-size: 10px;
    color: var(--text-2);
    width: 28px;
}

.circuit-3d-slider {
    flex-grow: 1;
    height: 12px;
}

.circuit-3d-toggle {
    font-size: 10px;
    color: var(--text-1);
    margin-top: 4px;
}

.circuit-3d-btn {
    margin-top: 6px;
    font-size: 10px;
    color: var(--text-1);
    background-color: rgba(16, 20, 28, 0.7);
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: var(--line-strong);
    border-bottom-color: var(--line-strong);
    border-left-color: var(--line-strong);
    border-right-color: var(--line-strong);
    padding: 4px 8px;
    border-radius: 6px;
}

.connections-panel {
    height: 14%;
    min-height: 120px;
    border-top-width: 1px;
    border-top-color: rgba(56, 189, 248, 0.2);
    background-color: var(--bg-0);
    padding: 10px 12px 12px 12px;
}

.connections-list {
    margin-top: 8px;
    flex-grow: 1;
    min-height: 0;
}

.connection-row {
    flex-direction: row;
    align-items: center;
    padding: 4px 6px;
    border-bottom-width: 1px;
    border-bottom-color: rgba(255, 255, 255, 0.05);
    min-width: 0;
}

.connection-row.header {
    background-color: rgba(15, 23, 42, 0.6);
    border-radius: 6px;
    border-bottom-width: 0;
    margin-bottom: 6px;
}

.connection-edit-btn {
    height: 18px;
    min-width: 44px;
    background-color: #2a3443;
    color: #d7e0ea;
    border-radius: 4px;
    font-size: 9px;
    padding-left: 6px;
    padding-right: 6px;
}

.connection-edit-btn:hover {
    background-color: #344155;
}

.connection-col-net {
    width: 70px;
    color: var(--text-2);
    font-size: 9px;
    -unity-font-style: bold;
}

.connection-col-nodes {
    flex-grow: 1;
    color: var(--text-1);
    font-size: 9px;
    white-space: nowrap;
    overflow: hidden;
    -unity-text-overflow-position: end;
}

.pin-net-dropdown {
    flex-grow: 1;
    margin-right: 6px;
    min-width: 90px;
}

.pin-net-readonly {
    flex-grow: 1;
    margin-right: 6px;
    min-width: 90px;
    background-color: var(--field-bg);
    border-top-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-color: var(--field-border);
    border-bottom-color: var(--field-border);
    border-left-color: var(--field-border);
    border-right-color: var(--field-border);
    border-radius: 6px;
    color: var(--text-0);
    font-size: 10px;
    padding: 4px 6px;
    -unity-text-align: middle-left;
    overflow: hidden;
    white-space: nowrap;
    -unity-text-overflow-position: end;
}

.pin-net-readonly.pin-net-unconnected {
    color: var(--text-2);
}

.pin-net-readonly.pin-net-connected {
    color: var(--ok);
}



.switch-state-dropdown .unity-base-field__input {
    color: #d7e0ea !important;
    border-color: transparent !important; 
    background-color: transparent !important;
}

.modal-overlay {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(10, 12, 16, 0.7);
    align-items: center;
    justify-content: center;
}

.modal-card {
    width: 420px;
    background-color: #141a22;
    border-radius: 10px;
    border-width: 1px;
    border-color: #243041;
    padding: 16px;
    flex-direction: column;
}

.modal-title {
    font-size: 14px;
    -unity-font-style: bold;
    color: #e6edf3;
    margin-bottom: 6px;
}

.modal-body {
    flex-direction: column;
}

.modal-label {
    font-size: 11px;
    color: #93a4b5;
    margin-top: 8px;
}

.modal-actions {
    flex-direction: row;
    justify-content: flex-end;
    margin-top: 12px;
}

.modal-actions .btn-primary {
    margin-left: 8px;
}

.net-editor-card {
    width: 520px;
}

.wire-export-card {
    width: 360px;
}

.wire-export-progress {
    margin-top: 8px;
    height: 10px;
}

.wire-export-progress .unity-progress-bar__background {
    background-color: #1b222b;
    border-width: 1px;
    border-color: #2b3440;
    border-radius: 6px;
}

.wire-export-progress .unity-progress-bar__progress {
    background-color: #3a8ee6;
    border-radius: 6px;
}

.wire-export-progress .unity-progress-bar__title {
    color: #c9d4e1;
    font-size: 10px;
}

.help-card {
    width: 680px;
}

.help-scroll {
    max-height: 380px;
    background-color: #0f141b;
    border-radius: 8px;
    padding: 8px 12px;
    margin-top: 6px;
    margin-bottom: 8px;
}

.help-section {
    color: #38bdf8;
    font-size: 11px;
    -unity-font-style: bold;
    margin-top: 6px;
}

.help-line {
    color: #cbd5f5;
    font-size: 10px;
    margin-top: 2px;
}

.net-editor-list {
    flex-grow: 1;
    max-height: 280px;
    background-color: #0f141b;
    border-radius: 8px;
    padding: 6px 16px;
    margin-top: 8px;
}

.net-editor-row {
    flex-direction: row;
    align-items: center;
    padding: 6px;
    border-bottom-width: 1px;
    border-bottom-color: rgba(255, 255, 255, 0.05);
}

.net-editor-row.delete {
    opacity: 0.5;
}

.net-editor-name {
    flex-grow: 1;
    margin-right: 8px;
}

.net-editor-count {
    width: 70px;
    color: #93a4b5;
    font-size: 9px;
    -unity-text-align: middle-right;
}

.net-editor-delete {
    margin-left: 8px;
    height: 18px;
    min-width: 50px;
    background-color: #3a1f1f;
    color: #f5c2c2;
    border-radius: 4px;
    font-size: 9px;
}

.net-editor-delete:hover {
    background-color: #4a2424;
}

.net-editor-conns-btn {
    margin-left: 8px;
    height: 18px;
    min-width: 50px;
    background-color: #2a3443;
    color: #d7e0ea;
    border-radius: 4px;
    font-size: 9px;
}

.net-editor-conns-btn:hover {
    background-color: #344155;
}

.net-editor-conns {
    background-color: #0b1016;
    border-radius: 8px;
    padding: 6px;
    margin-bottom: 6px;
}

.net-editor-foldout {
    margin-bottom: 6px;
    color: #c8d2de;
    font-size: 10px;
}

.net-editor-toggle {
    margin-left: 12px;
    font-size: 9px;
    color: #e6edf3;
}

.net-editor-toggle #unity-checkmark {
    width: 16px;
    height: 16px;
}

.net-editor-actions {
    flex-direction: row;
    align-items: center;
    margin-top: 12px;
}

.net-editor-error {
    color: #f59e0b;
    min-height: 14px;
}
