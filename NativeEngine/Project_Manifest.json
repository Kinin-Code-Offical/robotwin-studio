{
  "project": "LED_Blink",
  "mcu": {
    "model": "ATmega328P",
    "clock_hz": 16000000
  },
  "components": [
    { "id": "U1", "type": "ArduinoUno", "x": 100, "y": 200 },
    { "id": "R1", "type": "Resistor", "x": 400, "y": 200, "value": "220", "tolerance": "5%", "power": "0.25W" },
    { "id": "D1", "type": "LED", "x": 550, "y": 200, "vf": "2.0V", "if_max": "20mA" }
  ],
  "nets": [
    { "id": "NET_13", "nodes": ["U1.D13", "R1.P1"] },
    { "id": "NET_LED", "nodes": ["R1.P2", "D1.Anode"] },
    { "id": "NET_GND", "nodes": ["D1.Cathode", "U1.GND1"] }
  ],
  "init": {
    "node_gnd_voltage": 0.0,
    "node_vcc_voltage": 5.0
  }
}
